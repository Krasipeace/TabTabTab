{
  "name": "TabTabTab",
  "description": "This chrome extension makes it easier to manage your open tabs.",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "test": "vitest run",
    "lint": "eslint './src/**/*.{js,ts,jsx,tsx}' './test/**/*.{js,ts,jsx,tsx}'",
    "lint:fix": "eslint --fix './src/**/*.{js,ts,jsx,tsx}' './test/**/*.{js,ts,jsx,tsx}'",
    "format": "prettier './src/**/*.{js,ts,jsx,tsx}' './test/**/*.{js,ts,jsx,tsx}' --check",
    "format:fix": "prettier './src/**/*.{js,ts,jsx,tsx}' './test/**/*.{js,ts,jsx,tsx}' --write",
    "locale-lint": "chromex-locale-lint --localesDir dist/_locales"
  },
  "author": "okaryo",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.260",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "chromex-locale-lint": "^0.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.1",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.9",
    "@mui/material": "^5.15.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
